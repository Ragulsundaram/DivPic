{
  "manifest_version": 3,
  "name": "Element Screenshot",
  "version": "1.0",
  "description": "Capture screenshots of HTML elements with ease",
  "permissions": ["activeTab", "downloads"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }]
}